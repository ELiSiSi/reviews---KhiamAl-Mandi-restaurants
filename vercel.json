{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/app.js"
    }
  ],
  "functions": {
    "app.js": {
      "includeFiles": "views/**"
    }
  }
}
{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "functions": {
    "app.js": {
      "includeFiles": "views/**"
    }
  }
}
